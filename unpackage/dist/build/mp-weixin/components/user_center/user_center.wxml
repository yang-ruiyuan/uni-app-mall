<view class="userCenter"><view class="head style"><view class="personal-head-img"><image class="img" src="{{userInfo.avatarUrl||baseURL+'/weixinpl/shopping-temp/images/default.png'}}"></image></view><view class="personal-name"><view class="name">{{(userInfo.nickName||'')+"(ID:"+(userInfo.id||'未登录')+")"}}</view></view><view class="personal_card"><block wx:for="{{userInfo.rank}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="name"><image class="img" src="{{item.images}}"></image><text class="text">{{item.name}}</text></view></block></view></view><block wx:if="{{data.style.active==='style-1'}}"><block><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['GoTo',['$0'],[[['list','',index]]]]]]]}}" class="style-1" bindtap="__e"><view><image class="img" src="{{item.url}}"></image><view style="{{'color:'+(item.color)+';'}}">{{item.name}}</view></view><image class="right" src="{{baseURL+'/weixinpl/shopping-temp/images/arrows-right.png'}}"></image></view></block></block></block><block wx:if="{{data.style.active==='style-2'}}"><block><view class="style-2_c"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['GoTo',['$0'],[[['list','',index]]]]]]]}}" class="style-2" bindtap="__e"><image class="img" src="{{item.url}}"></image><view class="style-2-n" style="{{'color:'+(item.color)+';'}}">{{item.name}}</view></view></block></view></block></block></view>