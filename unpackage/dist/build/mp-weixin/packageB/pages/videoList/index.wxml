<view class="videoList"><block wx:for="{{video_list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><video class="video" poster="{{item.back_picture}}" vid="{{item.vid}}" autoplay="{{isfalse}}" playerid="{{'myVideo_'+item.id}}"></video><view class="middle"><view data-event-opts="{{[['tap',[['videoDetail',['$0'],[[['video_list','',index]]]]]]]}}" class="left" bindtap="__e">{{item.title}}</view><view class="right"><button class="box btn" open-type="share"><view class="share"></view></button><view class="box"><view data-event-opts="{{[['tap',[['myfavourite',['$0'],[[['video_list','',index]]]]]]]}}" class="{{['collect',[(item.user_status)?'act':'']]}}" bindtap="__e"></view><text>{{item.collect_count}}</text></view><view class="box"><view data-event-opts="{{[['tap',[['videoDetail',['$0'],[[['video_list','',index]]]]]]]}}" class="comment" bindtap="__e"></view><text>{{item.comment_count}}</text></view></view></view><view class="msg"><view class="msg_content"><view class="left">{{item.info}}</view></view></view></view></block></view>