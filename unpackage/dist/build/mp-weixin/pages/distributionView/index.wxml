<view class="myprofit"><block wx:if="{{Info.usermoney}}"><block><view class="top"><view><view class="total">{{"¥"+totoalMoney}}</view><view class="control"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" bindtap="__e">全部</view><view><picker mode="date" value="{{params.time}}" start="{{startDate}}" end="{{endDate}}" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" bindchange="__e"><view>{{params.time}}</view></picker></view></view><view class="num"><view><text>待结算</text><text>{{"¥"+Info.usermoney.total_profiy}}</text></view><view><text>已结算</text><text>{{"¥"+Info.usermoney.total_money}}</text></view></view></view></view><view class="main"><scroll-view class="srx" scroll-y data-event-opts="{{[['scrolltolower',[['loadMove',['$event']]]]]}}" bindscrolltolower="__e"><block wx:for="{{record}}" wx:for-item="item" wx:for-index="index" wx:key="index"><navigator class="item" url="{{'/pages/profitDetail/index?batchcode='+item.batchcode+'&id='+item.id}}" hover-class="none"><view class="name">{{item.own_user_name}}</view><view class="info"><view>{{item.createtime}}</view><view><uni-badge vue-id="{{'1-'+index}}" text="{{item.paytype}}" type="{{style[item.type]}}" bind:__l="__l"></uni-badge></view><view class="price">{{"¥"+item.reward}}</view></view></navigator></block></scroll-view><block wx:if="{{noData&&!record.length}}"><empty-data vue-id="2" bind:__l="__l" vue-slots="{{['default']}}">没有发现相关记录</empty-data></block></view><uni-drawer class="draw" vue-id="3" visible="{{rightDrawerVisible}}" mode="right" data-event-opts="{{[['^close',[['ToggleDrawer']]]]}}" bind:close="__e" bind:__l="__l" vue-slots="{{['default']}}"><view><view class="uni-card"><view class="uni-list"><block wx:for="{{lists}}" wx:for-item="list" wx:for-index="index" wx:key="index"><block><view class="uni-list-cell uni-collapse"><view data-event-opts="{{[['tap',[['trigerCollapse',[index]]]]]}}" class="{{['uni-list-cell-navigate uni-navigate-bottom',list.show?'uni-active':'']}}" bindtap="__